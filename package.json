{
  "name": "log4ngx-library",
  "version": "18.0.0",
  "description": "A Typescript logging framework for Angular projects",
  "author": "David Geary <david@secondbounce.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/secondbounce/log4ngx.git"
  },
  "bugs": {
    "url": "https://github.com/secondbounce/log4ngx/issues"
  },
  "keywords": [
    "angular",
    "log4",
    "logging",
    "typescript"
  ],
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build-lib": "ng build log4ngx",
    "build-demo": "ng build demo",
    "build": "npm run build-lib && npm run build-demo",
    "watch": "ng build log4ngx --watch --configuration development",
    "test": "ng test",
    "test-coverage": "ng test --no-watch --code-coverage",
    "lint": "ng lint"
  },
  "dependencies": {
    "@angular/animations": "^18.0.2",
